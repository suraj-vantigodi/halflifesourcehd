"LightmappedGeneric"
{
	// Original shader: BaseTimesLightmapPlusMaskedSphericalEnvMap
	"$detail" "detail/detail_noise1"
	"$detailscale" 2.5
	"$basetexture" "halflife/barrel4"
	"$envmapsphere" 1
	"$envmap" "environment maps/envmap001a"
	"$envmapmask" "halflife/barrel4r"
	"$surfaceprop" "metal_barrellight_hl"

	"$texscale"	4
	"$baseTextureOffset" "[0.5 0.5]"
	"Proxies"
	{
		"TextureTransform"
		{
			"translateVar" "$baseTextureOffset"
			"scaleVar"     "$texscale"
			"resultVar"    "$baseTextureTransform"
		}
	}
}
