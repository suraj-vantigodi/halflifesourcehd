"LightmappedGeneric"
{
	// Original shader: BaseTimesLightmap
	"$detail" "detail/detail_noise1"
	"$detailscale" 2.5
	"$basetexture" "halflife/subway_floor1"
	"$surfaceprop" "metal"
	"$envmap" "environment maps/envmap001a"
	"$basealphaenvmapmask" "1"
	"$envmapcontrast" 2
	"$envmaptint" "[ .8 .8 .8 ]"

	"$texscale"	4
	"$baseTextureOffset" "[0.5 0.5]"
    "Proxies"
     {
            "TextureTransform"
            {
		   "translateVar" "$baseTextureOffset"
		   "scaleVar"     "$texscale"
                   "resultVar"    "$baseTextureTransform"
            }
     }
}